{
    "name": "intelligent bookmark",
    "version": "1.0",
    "manifest_version": 2,
    "description": "search from your bookmarks and go to the desired page",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "bookmarks",
        "storage",
        "tabs"
    ],
    "background": {
        "scripts": ["bg.js"]
    }
}